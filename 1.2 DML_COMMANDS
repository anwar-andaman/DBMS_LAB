mysql> INSERT INTO INVOICE_MASTER VALUES(1,2,'2023-01-15');
Query OK, 1 row affected (0.05 sec)

mysql> INSERT INTO INVOICE_MASTER VALUES(2,1,'2023-01-17');
Query OK, 1 row affected (0.04 sec)

mysql> INSERT INTO INVOICE_MASTER VALUES(3,2,'2023-01-19');
Query OK, 1 row affected (0.10 sec)

mysql> INSERT INTO INVOICE_MASTER VALUES(4,7,'2023-01-11');
Query OK, 1 row affected (0.10 sec)

mysql> INSERT INTO INVOICE_MASTER VALUES(5,7,'2023-03-15');
Query OK, 1 row affected (0.04 sec)

mysql> INSERT INTO INVOICE_MASTER VALUES(6,4,'2023-02-15');
Query OK, 1 row affected (0.04 sec)

mysql> INSERT INTO INVOICE_MASTER VALUES(7,2,'2023-03-11');
Query OK, 1 row affected (0.04 sec)

mysql> SELECT *FROM INVOICE_MASTER;
+--------+---------+------------+
| INV_ID | CUST_ID | INV_DATE   |
+--------+---------+------------+
|      1 |       2 | 2023-01-15 |
|      2 |       1 | 2023-01-17 |
|      3 |       2 | 2023-01-19 |
|      4 |       7 | 2023-01-11 |
|      5 |       7 | 2023-03-15 |
|      6 |       4 | 2023-02-15 |
|      7 |       2 | 2023-03-11 |
+--------+---------+------------+
7 rows in set (0.00 sec)

mysql> INSERT INTO INVOICE_ITEM VALUES(1,1,5);
Query OK, 1 row affected (0.08 sec)

mysql> INSERT INTO INVOICE_ITEM VALUES(1,2,1);
Query OK, 1 row affected (0.04 sec)

mysql> INSERT INTO INVOICE_ITEM VALUES(3,2,2);
Query OK, 1 row affected (0.12 sec)

mysql> INSERT INTO INVOICE_ITEM VALUES(3,3,1);
Query OK, 1 row affected (0.05 sec)

mysql> INSERT INTO INVOICE_ITEM VALUES(3,7,3);
Query OK, 1 row affected (0.04 sec)

mysql> INSERT INTO INVOICE_ITEM VALUES(1,7,4);
Query OK, 1 row affected (0.04 sec)

mysql> INSERT INTO INVOICE_ITEM VALUES(4,5,1);
Query OK, 1 row affected (0.04 sec)

mysql> SELECT *FROM INVOICE_ITEM;
+--------+------+----------+
| INV_ID | PID  | QUANTITY |
+--------+------+----------+
|      1 |    1 |        5 |
|      1 |    2 |        1 |
|      3 |    2 |        2 |
|      3 |    3 |        1 |
|      3 |    7 |        3 |
|      1 |    7 |        4 |
|      4 |    5 |        1 |
+--------+------+----------+
7 rows in set (0.00 sec)

