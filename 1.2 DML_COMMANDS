  3.      Insert values in INVOICE database. 
  
mysql> INSERT INTO CUSTOMER VALUES(1,'MISHAB','KALANTHODE, KOZHIKODE, KERALA','KOZHIKODE',8767543421);
Query OK, 1 row affected (0.04 sec)

mysql> INSERT INTO CUSTOMER VALUES(2,'RAM','VALANCHERI, MALAPPURAM, KERALA','MALAPPURAM',8767988654);
Query OK, 1 row affected (0.06 sec)

mysql> INSERT INTO CUSTOMER VALUES(3,'ABHISHEK','MINNIE BAY, PORT BLAIR, ANDAMAN','PORT BLAIR',9878654532);
Query OK, 1 row affected (0.08 sec)

mysql> INSERT INTO CUSTOMER VALUES(4,'SUNIL KUMAR','GARACHARMA, PORT BLAIR, ANDAMAN','PORT BLAIR',9933257865);
Query OK, 1 row affected (0.08 sec)

mysql> INSERT INTO CUSTOMER VALUES(5,'BABITA','ASHOK NAGAR, NEW DELHI, NCT OF DELHI','NEW DELHI',8767565432);
Query OK, 1 row affected (0.04 sec)

mysql> INSERT INTO CUSTOMER VALUES(6,'ASAD AKBAR','ELECTRONIC CITY, BANGLURU, KARNATAKA','BANGLURU',9878653421);
Query OK, 1 row affected (0.04 sec)

mysql> INSERT INTO CUSTOMER VALUES(7,'SAVITA','NEAR DARGAH, AJMER, RAJASTHAN','AJMER',7890564532);
Query OK, 1 row affected (0.04 sec)

